  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev_server dev_extension",
    "build": "npm-run-all clean build_server build_extension",
    "start": "cross-env NODE_ENV=production TS_NODE_PROJECT=server/tsconfig.json ts-node server/index.ts",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "clean": "rm -rf dist",
    "build_server": "nuxt-ts build",
    "build_extension": "cross-env TS_NODE_PROJECT=server/tsconfig.json webpack",
    "dev_server": "cross-env NODE_ENV=development TS_NODE_PROJECT=server/tsconfig.json nodemon server/index.ts --watch server",
    "dev_extension": "cross-env TS_NODE_PROJECT=server/tsconfig.json webpack --watch"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.5",
    "@nuxtjs/axios": "^5.9.7",
