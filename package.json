    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "nuxt": "^2.12.2",
    "nuxt-buefy": "^0.3.26",
    "replace-in-file": "^5.0.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.5",
    "@nuxtjs/eslint-config": "^2.0.2",
